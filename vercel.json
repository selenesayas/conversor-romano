{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "public/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/a2r", "dest": "/api/a2r.js" },
    { "src": "/r2a", "dest": "/api/r2a.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}



